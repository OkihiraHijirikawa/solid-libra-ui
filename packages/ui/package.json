{
  "name": "solid-libra-ui",
  "version": "0.2.1",
  "type": "module",
  "private": false,
  "description": "collection of accessible and customizable UI components for SolidJS, designed to be themed with Tailwind CSS.",
  "bin": {
    "solid-libra-ui": "scripts/cli.mjs"
  },
  "files": [
    "scripts",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:init": "node ./scripts/init-ui.mjs",
    "dev:add": "node ./scripts/add-component.mjs"
  },
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "components",
    "headless",
    "tailwindcss"
  ],
  "author": "OkihiraHijirikawa (https://github.com/OkihiraHijirikawa/solid-libra-ui.git)",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "class-variance-authority": "^0.7.1",
    "solid-icons": "^1.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "solid-js": "^1.9.9",
    "tailwindcss": "^4.0.7"
  }
}
